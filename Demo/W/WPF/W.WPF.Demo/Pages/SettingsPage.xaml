<pages:Page x:Class="W.WPF.Demo.Pages.SettingsPage"
             x:TypeArguments="models:SettingsModel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:localPages="clr-namespace:W.WPF.Demo.Pages"
             xmlns:models="clr-namespace:W.WPF.Demo.Models"
             xmlns:pages="clr-namespace:W.WPF.Pages;assembly=Tungsten.WPF"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             >
    <Grid>
         <TextBlock Text="{Binding Title.Value}" HorizontalAlignment="Center" VerticalAlignment="Center" />
   </Grid>
</pages:Page>
